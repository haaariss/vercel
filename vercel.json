{
    "version": 2,
    "builds": [
      {
        "src": "a1//wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "a1//wsgi.py"
      },
        {
  "build": {
    "env": {
      "PYTHON_INSTALL_RUNTIME_DEPS": "true"
    }
  }
}
    ]
  }
